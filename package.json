{
  "name": "dembe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev-server start-csr-dev",
    "dev-server": "webpack-dev-server --config config/webpack.dev.js",
    "start-csr-dev": "better-npm-run start-csr-dev",
    "clean": "rimraf ./public/build",
    "prebuild": "npm run clean",
    "build": "npm-run-all build-client build-server",
    "build-client": "better-npm-run build-client",
    "build-server": "better-npm-run build-server",
    "start": "better-npm-run start-csr-prod",
    "start-ssr": "better-npm-run start-ssr-prod"
  },
  "betterScripts": {
    "start-csr-dev": {
      "command": "node ./server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-csr-prod": {
      "command": "node ./server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-ssr-prod": {
      "command": "node ./server/index.js",
      "env": {
        "NODE_ENV": "production",
        "SSR": 1
      }
    },
    "build-client": {
      "command": "webpack --config config/webpack.prod.js --progress --display-optimization-bailout",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-server": {
      "command": "webpack --config config/webpack.server.js --progress --display-optimization-bailout",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "6.23.0",
    "better-npm-run": "0.0.15",
    "compression": "1.6.2",
    "express": "4.15.3",
    "http-proxy-middleware": "0.17.4",
    "isomorphic-fetch": "2.2.1",
    "npm-run-all": "4.0.2",
    "object-assign": "4.1.1",
    "promise": "8.0.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-apollo": "1.4.3",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-universal-component": "1.2.1",
    "redux": "3.7.1",
    "redux-promise-middleware": "4.3.0",
    "serialize-javascript": "1.3.0",
    "serve-favicon": "2.4.3",
    "webpack-flush-chunks": "1.1.10"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "1.0.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.4",
    "eslint": "4.2.0",
    "eslint-config-react-app": "1.0.5",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-css-chunks-webpack-plugin": "2.0.3",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
